{
    "styleSheets": [
        "theme.css"
    ],
    "configOverrides": {
        "progressIndicator": {
            "inlineMode": false
        },
        "videoContainer": {
            "overPlaybackBar": false
        },
        "plugins": {
            "es.upv.paella.volumeButtonPlugin": {
                "volumeAlwaysVisible": false
            }
        }
    },
    "icons": [
        { "plugin": "es.upv.paella.playPauseButton", "identifier": "play", "icon": "./icons/play.svg" },
        { "plugin": "es.upv.paella.playPauseButton", "identifier": "pause", "icon": "./icons/pause.svg" },
        { "plugin": "es.upv.paella.playPauseButton", "identifier": "replay", "icon": "./icons/replay.svg" },

        { "plugin": "es.upv.paella.backwardButtonPlugin","identifier": "backwardIcon", "icon": "./icons/seek-backward.svg"},
        { "plugin": "es.upv.paella.forwardButtonPlugin","identifier": "forwardIcon", "icon": "./icons/seek-forward.svg"},

        { "plugin": "es.upv.paella.volumeButtonPlugin","identifier": "volumeHighIcon", "icon": "./icons/volume-high.svg"},
        { "plugin": "es.upv.paella.volumeButtonPlugin","identifier": "volumeMidIcon", "icon": "./icons/volume-low.svg"},
        { "plugin": "es.upv.paella.volumeButtonPlugin","identifier": "volumeLowIcon", "icon": "./icons/volume-low.svg"},
        { "plugin": "es.upv.paella.volumeButtonPlugin","identifier": "volumeMuteIcon", "icon": "./icons/mute.svg"},

        { "plugin": "es.upv.paella.fullscreenButton", "identifier": "fullscreenIcon", "icon": "./icons/fullscreen.svg" },
        { "plugin": "es.upv.paella.fullscreenButton", "identifier": "windowedIcon", "icon": "./icons/fullscreen-exit.svg" },

        { "plugin": "es.upv.paella.captionsSelectorPlugin", "identifier": "captionsIcon", "icon": "./icons/subtitles.svg" },

        
        { "plugin": "org.opencast.paella.toolsGroupPlugin", "identifier": "buttonIcon", "icon": "./icons/settings.svg" },
        { "plugin": "org.opencast.paella.descriptionPlugin", "identifier": "buttonIcon", "icon": "./icons/info.svg" },
        { "plugin": "es.upv.paella.downloadsPlugin", "identifier": "downloadIcon", "icon": "./icons/download.svg" },
        { "plugin": "org.opencast.paella.versionButton", "identifier": "buttonIcon", "icon": "./icons/bookmark.svg" },
        

        { "plugin": "org.opencast.paella.loginPlugin", "identifier": "buttonIcon", "icon": "./icons/user.svg" },
        { "plugin": "org.opencast.paella.episodesFromSeries", "identifier": "buttonIcon", "icon": "./icons/playlist.svg" }    
        
    ],

    "icons_not_used": [
        { "plugin": "************ es.upv.paella.layoutSelector","identifier": "layoutIcon", "icon*": "./icons/presentation-mode.svg"},
        { "plugin": "************ es.upv.paella.keyboardShortcutsHelp", "identifier": "keyboardIcon", "icon*": "./icons/download.svg" },
        { "plugin": "************ org.opencast.paella.editorPlugin", "identifier": "buttonIcon", "icon*": "./icons/bookmark.svg" }
    ]

}